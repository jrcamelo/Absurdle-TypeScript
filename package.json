{
  "name": "wordle",
  "version": "1.0.0",
  "description": "Wordle game with Absurdle variation",
  "author": "Camelo <jr@camelo.dev> (@jrcamelo)",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint-fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "nodemon --ext ts --exec \"jasmine-ts  --config=./jasmine.json\""
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/jasmine": "3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine": "3.4.0",
    "jasmine-core": "^4.0.0",
    "jasmine-ts": "0.3.2",
    "jasmine-ts-console-reporter": "3.1.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "12.3.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  }
}
