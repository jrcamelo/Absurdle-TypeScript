{
  "name": "secretle-api",
  "version": "1.0.0",
  "description": "API for a Wordle variation with a focus on no-cheating gameplay.",
  "author": "Camelo <jr@camelo.dev> (@jrcamelo)",
  "keywords": [
    "wordle",
    "game",
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint-fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "nodemon --ext ts --exec \"jasmine-ts  --config=./jasmine.json\""
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@types/shuffle-seed": "1.1.0",
    "@types/swagger-ui-react": "4.1.1",
    "@types/uuid": "8.3.4",
    "dotenv": "16.0.0",
    "eslint-plugin-import": "*",
    "hi-base32": "0.5.1",
    "mongodb": "4.3.1",
    "mongoose": "6.2.1",
    "next": "12.0.10",
    "next-swagger-doc": "0.1.12",
    "openapi-types": "10.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "shuffle-seed": "1.1.6",
    "swagger-ui-react": "4.5.0",
    "tsconfig-paths": "3.12.0",
    "uuid": "8.3.2",
    "winston": "3.5.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.16",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine": "3.4.0",
    "jasmine-core": "^4.0.0",
    "jasmine-ts": "0.3.2",
    "jasmine-ts-console-reporter": "3.1.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "12.3.3",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
